{
    "data": {
        "train_path": "../input/cat_in_dat/train.csv",
        "test_path": "../input/cat_in_dat/test.csv",
        "output_dir": ".",
        "type": "table",
        "pred_cols": ["target"],
        "id_col": "id",
        "delimiter": ","
    },
    "pre": {
        "train_mode": "clf",
        "table": {
            "adhoc_df": {
                "myfunc": "cat_in_dat",
                "methods": [
                    "add_ord_1_label",
                    "add_ord_2_label",
                    "add_ord_3_label",
                    "add_ord_4_label",
                    "split_ord_5"
                ]
            },
            "deletion": [
                "bin_0",
                "ord_1",
                "ord_2",
                "ord_3",
                "ord_4"
            ],
            "category_encoding": {
                "default": {
                    "model": "onehot_with_test"
                },
                "options": [
                    {
                        "columns": [
                            "bin_0",
                            "bin_1",
                            "bin_2",
                            "day",
                            "month"
                        ],
                        "model": "onehot_with_test"
                    }
                ]
            },
            "x_scaler": "standard"
        },
        "image": {
        }
    },
    "fit": {
        "train_mode": "clf",
        "scoring": "roc_auc",
        "cv": {
            "fold": "stratifiedk",
            "num": 5
        },
        "ensemble_model_config": {
            "mode": "average",
            "model": "log_reg",
            "modelname": "cat_in_dat",
            "k": 10,
            "proportion": 0.2
        },
        "single_model_configs": [
            {
                "model": "log_reg",
                "modelname": "log_reg_1",
                "cv_select": "train_all",
                "n_trials": 1,
                "fit_params": {
                },
                "params": {
                    "C": [0.09536298444122952],
                    "class_weight": ["balanced"],
                    "max_iter": [10000],
                    "solver": ["lbfgs"],
                    "n_jobs": [-1]
                }
            }
        ]
    },
    "post": {
        "myfunc": "cat_in_dat",
        "methods": [
            "translate_proba2submission"
        ]
    },
    "notify": {
        "slack": "https://hooks.slack.com/services/hogehoge"
    }
}
