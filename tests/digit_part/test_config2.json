{
    "data": {
        "train_path": "./tests/digit_part/train.csv",
        "test_path": "./tests/digit_part/test.csv",
        "output_dir": "./outputs",
        "type": "table",
        "pred_cols": ["label"],
        "id_col": "ImageId"
    },
    "pre": {
        "train_mode": "clf",
        "table": {
            "adhoc_df": {
                "myfunc": "digit",
                "methods": [
                    "add_image_id"
                ]
            },
            "missing_imputation": true,
            "category_encoding": {
                "default": {
                    "model": "onehot"
                },
                "options": [
                ]
            },
            "x_scaler": "standard",
            "dimension_reduction": {
                "n": 100,
                "model": "pca"
            },
            "reshape_for_keras": "lstm"
        }
    },
    "fit": {
        "myfunc": "digit",
        "train_mode": "clf",
        "scoring": "accuracy",
        "cv": {
            "fold": "stratifiedk",
            "num": 3
        },
        "modelname": "tmp_digit2",
        "ensemble_model_config": {
            "mode": "stacking",
            "model": "log_reg",
            "k": 10
        },
        "single_model_configs": [
            {
                "model": "keras_clf",
                "cv_select": "train_all",
                "fit_params": {
                    "epochs": 3,
                    "batch_size": 50
                },
                "params": {
                }
            }
        ]
    },
    "post": {
        "myfunc": "digit",
        "methods": [
            "translate_label2upper"
        ]
    },
    "notify": {
        "slack": "https://hooks.slack.com/services/hogehoge"
    }
}
